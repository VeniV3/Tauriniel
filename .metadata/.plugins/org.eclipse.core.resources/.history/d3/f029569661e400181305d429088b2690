package me.veni;

import java.util.Scanner;

public class Extension {
	
	Scanner s;
	
	public Extension() {
		s = new Scanner(System.in);
		roar();
	}
	
	private void roar() {
		if(s.nextLine() == "chuj") {
			System.out.println("good");
			
		}else if(s.nextLine() == "dwachuje"){
			
			
		}
		
	}
	//konkretny ci¹g znaków
}
